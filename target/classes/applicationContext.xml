<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- bean definitions here -->
	<bean name="add" class="com.springcore.model.Address">
	<property name="city" value ="Noida"/>
	<property name="state" value="UP"/>
	
	
	</bean>

	<bean name="emp" class="com.springcore.model.Employee">
		<property name="id" value="111" />
		<property name="name" value="Guest" />
		<property name="gender" value="Male" />
		<property name="address" ref="add" /> 
		
		<property name = "list">
		<list>
		<value>103</value>
		<value>101</value>
		<value>199</value>
		<value>143</value>
		<value>156</value>
		<value>103</value>
		</list>
		</property>
		
		<property name="set">
		
		<set>
	    <value>103</value>
		<value>166</value>
		<value>167</value>
		<value>188</value>
		<value>197</value>
		<value>103</value>
		</set>	
		</property>
		
		<property name="map">
		
		<map>
		<entry key ="1" value="one"/>
			<entry key ="2" value="two"/>
			<entry key ="3" value="three"/>
			<entry key ="4" value="four"/>
				<entry key ="1" value="oneee"/>
		</map>	
		</property>
		 		
		
	</bean>



